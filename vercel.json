{
  "version": 2,
  "buildCommand": "cd packages/frontend && npm run build",
  "devCommand": "cd packages/frontend && npm run dev -- --port $PORT",
  "outputDirectory": "packages/frontend/dist",
  "functions": {
    "packages/frontend/api/**/*.ts": {
      "maxDuration": 30,
      "runtime": "nodejs20.x"
    }
  },
  "env": {
    "FORCE_RUNTIME_TAG": "2025-01-30-fix-v3",
    "FORCE_REBUILD": "true"
  }
}